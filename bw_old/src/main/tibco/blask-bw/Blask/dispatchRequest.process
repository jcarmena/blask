<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.w3.org/1999/xhtml" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions">
    <xsd:import namespace="http://www.w3.org/1999/xhtml" schemaLocation="/Blask/Schemas/xhtml5_with_microdata.xsd"/>
    <pd:name>Blask/dispatchRequest.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="request">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="protocol" type="xsd:string"/>
                    <xsd:element name="port" type="xsd:string"/>
                    <xsd:element name="HTTPVersion" type="xsd:string"/>
                    <xsd:element name="method" type="xsd:string"/>
                    <xsd:element name="requestURI" type="xsd:string"/>
                    <xsd:element name="command" type="xsd:string"/>
                    <xsd:element name="query" type="xsd:string"/>
                    <xsd:element name="Header" minOccurs="0" maxOccurs="unbounded">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="name" type="xsd:string"/>
                                <xsd:element name="value" type="xsd:string"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="postData" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>107</pd:startX>
    <pd:startY>156</pd:startY>
    <pd:returnBindings>
        <response>
            <xsl:choose>
                <xsl:when test="exists($Call-view)">
                    <content>
                        <xsl:value-of select="$Call-view/response/content"/>
                    </content>
                </xsl:when>
                <xsl:otherwise>
                    <content>
                        <xsl:value-of select="$Call-default-view/response/content"/>
                    </content>
                </xsl:otherwise>
            </xsl:choose>
        </response>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endX>1008</pd:endX>
    <pd:endY>154</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1522539614554</pd:targetNamespace>
    <pd:activity name="route">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>329</pd:x>
        <pd:y>155</pd:y>
        <config>
            <processName>/Blask/Routing/route.process</processName>
        </config>
        <pd:inputBindings>
            <input>
                <method>
                    <xsl:value-of select="$Start/request/method"/>
                </method>
                <path>
                    <xsl:value-of select="$Start/request/command"/>
                </path>
            </input>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Call view">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>746</pd:x>
        <pd:y>153</pd:y>
        <config>
            <processName>/Blask/Routing/defaultView.process</processName>
            <processNameXPath>$route/result/process</processNameXPath>
        </config>
        <pd:inputBindings>
            <input/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Set Request Context">
        <pd:type>com.tibco.pe.core.SetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.setSharedVariable</pd:resourceType>
        <pd:x>568</pd:x>
        <pd:y>160</pd:y>
        <config>
            <variableConfig>/Blask/requestContext.jobsharedvariable</variableConfig>
        </config>
        <pd:inputBindings>
            <requestContext>
                <method>
                    <xsl:value-of select="$Start/request/method"/>
                </method>
                <command>
                    <xsl:value-of select="$Start/request/command"/>
                </command>
                <xsl:for-each select="$route/result/parameter">
                    <parameter>
                        <name>
                            <xsl:value-of select="name"/>
                        </name>
                        <value>
                            <xsl:value-of select="value"/>
                        </value>
                    </parameter>
                </xsl:for-each>
                <xsl:for-each select="tib:tokenize($Start/request/query, &quot;&amp;&quot;)">
                    <args>
                        <name>
                            <xsl:value-of select="substring-before(current(), &quot;=&quot;)"/>
                        </name>
                        <value>
                            <xsl:value-of select="substring-after(current(), &quot;=&quot;)"/>
                        </value>
                    </args>
                </xsl:for-each>
                <xsl:if test="$route/result/key">
                    <routeKey>
                        <xsl:value-of select="$route/result/key"/>
                    </routeKey>
                </xsl:if>
            </requestContext>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Null">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>692</pd:x>
        <pd:y>304</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Set Response">
        <pd:type>com.tibco.pe.core.SetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.setSharedVariable</pd:resourceType>
        <pd:x>446</pd:x>
        <pd:y>154</pd:y>
        <config>
            <variableConfig>/Blask/Response/response.jobsharedvariable</variableConfig>
        </config>
        <pd:inputBindings>
            <response>
                <statusCode>
                    <xsl:value-of select="200"/>
                </statusCode>
            </response>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>737</pd:x>
        <pd:y>443</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Set Not found response">
        <pd:type>com.tibco.pe.core.SetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.setSharedVariable</pd:resourceType>
        <pd:x>831</pd:x>
        <pd:y>312</pd:y>
        <config>
            <variableConfig>/Blask/Response/response.jobsharedvariable</variableConfig>
        </config>
        <pd:inputBindings>
            <response>
                <statusCode>
                    <xsl:value-of select="404"/>
                </statusCode>
            </response>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Get App Context">
        <pd:type>com.tibco.pe.core.GetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.getSharedVariable</pd:resourceType>
        <pd:x>222</pd:x>
        <pd:y>158</pd:y>
        <config>
            <variableConfig>/Blask/appContext.sharedvariable</variableConfig>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="createErrorResponse">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>915</pd:x>
        <pd:y>443</pd:y>
        <config>
            <processName>/Blask/Response/createErrorResponse.process</processName>
        </config>
        <pd:inputBindings>
            <input>
                <xsl:for-each select="$_error/ns:ErrorReport">
                    <ErrorReport>
                        <StackTrace>
                            <xsl:value-of select="StackTrace"/>
                        </StackTrace>
                        <Msg>
                            <xsl:value-of select="Msg"/>
                        </Msg>
                        <FullClass>
                            <xsl:value-of select="FullClass"/>
                        </FullClass>
                        <Class>
                            <xsl:value-of select="Class"/>
                        </Class>
                        <ProcessStack>
                            <xsl:value-of select="ProcessStack"/>
                        </ProcessStack>
                        <xsl:if test="MsgCode">
                            <MsgCode>
                                <xsl:value-of select="MsgCode"/>
                            </MsgCode>
                        </xsl:if>
                    </ErrorReport>
                </xsl:for-each>
            </input>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Get App Context</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>route</pd:from>
        <pd:to>Set Response</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Call view</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Set Request Context</pd:from>
        <pd:to>Call view</pd:to>
        <pd:xpathDescription>Route OK</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>exists($route/result/process)</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Set Request Context</pd:from>
        <pd:to>Null</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Set Response</pd:from>
        <pd:to>Set Request Context</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>createErrorResponse</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Null</pd:from>
        <pd:to>Set Not found response</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Set Not found response</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Get App Context</pd:from>
        <pd:to>route</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>createErrorResponse</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>